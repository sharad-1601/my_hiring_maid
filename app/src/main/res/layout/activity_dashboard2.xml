<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".dashboard2"
    android:background="@color/material_dynamic_neutral80"
    android:orientation="vertical">



    <ImageView
        android:id="@+id/maid"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/logohome"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="-1dp" />

    <Button
        android:id="@+id/verify_mobile"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="verify mobile"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="472dp" />

    <Button
        android:id="@+id/payment_maid"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="payment"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="593dp" />

    <Button
        android:id="@+id/attendance_maid"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="attendance"
        tools:layout_editor_absoluteX="-16dp"
        tools:layout_editor_absoluteY="657dp" />



    <Button
        android:id="@+id/feedback_maid"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Feedback"
        tools:layout_editor_absoluteX="164dp"
        tools:layout_editor_absoluteY="530dp" />
</LinearLayout>